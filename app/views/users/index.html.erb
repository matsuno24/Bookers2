<%= render 'userinfo', user: @user, book: @book %>


<h2>Users</h2>
<table>
    <tr>
        <th>image</th>
        <th>name</th>
    </tr>
    <% @users.each do |users| %>
        <tr>
          <td><%= image_tag users.get_image(100,100) %></td>
          <td><%= users.name %></td>
          <td><%= link_to 'Show', user_path(users.id) %></td>
        </tr>
    <% end %>
</table>